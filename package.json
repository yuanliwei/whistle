{
  "name": "whistle",
  "description": "HTTP, HTTP2, HTTPS, Websocket debugging proxy",
  "version": "2.9.64",
  "dataDirname": ".whistle",
  "localUIHost": "local.whistlejs.com",
  "port": 8899,
  "sockets": 256,
  "timeout": 360000,
